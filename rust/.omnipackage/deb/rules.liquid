#!/usr/bin/make -f
# debian.rules
# Be carefull, this is a Makefile, so the indents are tabs and not spaces!
# -*- makefile -*-

export DESTROOT=$(CURDIR)/debian/{{ package_name }}
# export DH_VERBOSE=1

%:
	dh $@ --parallel

override_dh_auto_build:
	cargo build --release

override_dh_auto_install:
	install -d -m755 $(DESTROOT)/usr/bin/
	install -m755 target/release/rust $(DESTROOT)/usr/bin/omnipackage_example_rust
